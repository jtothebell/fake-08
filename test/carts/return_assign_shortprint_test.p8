local ol = split"\|f,\-f,\-h,\+fh,\+hh,\|i,\|h"
function outlined_text(s, x, y, c1, c2)
	for p in all(ol) do
	 ?p..s, x, y, c1
	end

	thing=?s, x, y, c2
	--return thing
	return ?s, x, y, c2
end

function _draw()
 cls()
 local res1=outlined_text("all levels unlocked!", 12, 106, 4, 13)
 print(res1)
 local res2=?"all levels unlocked!", 0, 0, 4, 13
 print(res2)
end
